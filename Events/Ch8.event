EndingScene12:
MUSC 0x31 //victory

CHECK_ALIVE Ersatz
BEQ 0x4 rC r0 // Skip block if Ersatz is dead
    SetBackground(0x23)
    Text(ChEightEnd)
    TurnAlly(Ersatz)
    GOTO 0x5
LABEL 0x4
    SetBackground(0x23)
    Text(ChEightEndNoErsatz)
    GiveMoney(5000)
LABEL 0x5

CHECK_ALIVE Fanni
BEQ 0xb rC r0 //Skip if Fanni is dead.
    TurnAlly(Fanni)
LABEL 0xb

FADI 5
MNCH 0x9
ENDA

