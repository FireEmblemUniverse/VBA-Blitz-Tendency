// r1-r4 are pointers to units.
LoadOneOfFour:
    RANDOMNUMBER 0x4       //Sets slot C to a number from 0 to 4.
    SMOV r5 0x0
    BEQ 0x0 rC r5
    SMOV r5 1
    BEQ 0x1 rC r5
    SMOV r5 2
    BEQ 0x2 rC r5

    SADD slotsParam(r2, r4, r0)
    CALL $9EE274
    GOTO 3

    LABEL 0
    SADD slotsParam(r2, r1, r0)
    CALL $9EE274
    GOTO 3

    LABEL 1
    CALL $9EE274
    GOTO 3

    LABEL 2
    SADD slotsParam(r2, r3, r0)
    CALL $9EE274

    LABEL 3
ENDA
















