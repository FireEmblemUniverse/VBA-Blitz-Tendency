// Those are tile change lists to be used with the ASMC
// I use the asmc instead of the regular TILECHANGE/TILEREVERT events
// because I want to display the tile change fade for more than one at the time
// otherwise it would look kind of weird (especially for Shift7, that's in 2 parts)

// Format is SHORT <mapChangeId> <Bool: True => Trigger, False => Revert>

mclShift1:
	SHORT 10 False
	SHORT 20 True
	SHORT 21 True
	SHORT (-1)

mclShift2:
	SHORT 20 False
	SHORT 21 False
	SHORT 30 True
	SHORT (-1)

mclShift3:
	SHORT 30 False
	SHORT 40 True
	SHORT 41 True
	SHORT (-1)

mclShift4:
	SHORT 40 False
	SHORT 41 False
	SHORT 50 True
	SHORT 51 True
	SHORT (-1)

mclShift5:
	SHORT 50 False
	SHORT 51 False
	SHORT 60 True
	SHORT (-1)

mclShift6:
	SHORT 60 False
	SHORT 70 True
	SHORT 71 True
	SHORT (-1)

mclShift7:
	SHORT 70 False
	SHORT 71 False
	SHORT 80 True
	SHORT 81 True
	SHORT (-1)

mclShift8:
	SHORT 80 False
	SHORT 81 False
	SHORT 90 True
	SHORT (-1)

mclShift9:
	SHORT 90  False
	SHORT 100 True
	SHORT 101 True
	SHORT (-1)

mclShift10:
	SHORT 100 False
	SHORT 101 False
	SHORT 110 True
	SHORT 111 True
	SHORT (-1)

mclShift11:
	SHORT 110 False
	SHORT 111 False
	SHORT 120 True
	SHORT 121 True
	SHORT (-1)

mclShift12:
	SHORT 120 False
	SHORT 121 False
	SHORT 10  True
	SHORT (-1)

